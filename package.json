		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.4",
		"typescript": "^4.7.2",
		"vite": "^2.9.13"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.0.1",
		"env-cmd": "^10.1.0"
